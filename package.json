		"preview": "vite preview",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,ts,tsx,json}": "biome check --write"
	},
	"dependencies": {
		"@imgly/background-removal": "^1.7.0",
		"@radix-ui/react-context-menu": "^2.2.16",
